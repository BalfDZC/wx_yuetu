.title {
  font-size: 40rpx;
  padding: 30rpx;
}

.text-right {
  font-size: 40rpx;
  color: #ccc;
  text-align: right;
  padding-right: 30rpx;
}

.header-image {
  margin-top: 10rpx;
  width: 750rpx;
  height: 400rpx;
  display: block;
}

.text_image {
  width: 750rpx;
  height: 640rpx;
}

.text_main {
  /* border:1rpx solid #ccc; */
  margin-top: 10rpx;
}

.pushText_header {
  height: 50rpx;
  padding-left: 10rpx;
  /* background-color: pink; */
  border-left: 4rpx solid red;
  margin: 40rpx 20rpx;
}

.division {
  height: 20rpx;
  background-color: #ccc;
}

.pushText_mian {
  /* display: flex; */
  height: 250rpx;
  overflow: hidden;
}

.pushText_mian_left {
  float: left;
  padding: 10rpx 10rpx 0 10rpx;
  width: 350rpx;
  /* flex: 1; */
}

.pushText_mian_left image {
  float: left;
  width: 100%;
  /* flex: 1;s */
}

.pushText_mian_right {
  padding-top: 10rpx;
  float: left;
  width: 300rpx;
}

.suspend {
  position: fixed;
  width: 600rpx;
  bottom: 20rpx;
  right: 10%;
  height: 80rpx;
  background-color: #393b3d;
  display: flex;
  border-radius: 80rpx;
  line-height: 80rpx;
  overflow: hidden;
}

.suspend_left {
  width: 300rpx;
  color: #fff;
  height: 60rpx;
  /* padding: 0  40rpx 0 80rpx; */
  text-align: center;
}

.suspend_center {
  margin-top: 18rpx;
  width: 2rpx;
  height: 50rpx;
  background-color: #ccc;
}

.suspend_right {
  background-color: #393b3d;
  width: 300rpx;
  text-align: center;
  line-height: 80rpx;
  color: #01b56d;
}

.text_main_title {
  padding: 0 10rpx 10rpx 0;
}

@font-face {
  font-family: 'iconfont'; /* project id 1189425 */
